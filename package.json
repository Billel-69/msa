{
  "name": "msa-project",
  "version": "1.0.0",
  "description": "MSA Educational Platform - Complete Testing Suite",
  "scripts": {
    "install:all": "npm ci && cd backend && npm ci && cd ../frontend && npm ci",
    "test:all": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm run test:ci",
    "test:integration": "cd backend && npm run test:integration",
    "test:e2e": "cypress run",
    "lint:all": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npx eslint . --ext .js",
    "lint:frontend": "cd frontend && npm run lint",
    "build:all": "npm run build:backend && npm run build:frontend",
    "build:backend": "echo 'Backend build completed (Node.js)'",
    "build:frontend": "cd frontend && npm run build",
    "docker:build": "docker-compose -f docker-compose.test.yml build",
    "docker:test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "docker:down": "docker-compose -f docker-compose.test.yml down -v",
    "jenkins:setup": "echo 'Setting up Jenkins pipeline...'",
    "precommit": "npm run lint:all && npm run test:all"
  },
  "devDependencies": {
    "cypress": "^13.0.0",
    "jest": "^29.0.0",
    "jest-junit": "^16.0.0",
    "supertest": "^6.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/msa.git"
  },
  "license": "MIT"
}