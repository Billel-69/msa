# Fichier: .env
# Configuration pour réseau local - PC principal

# ===== RÉSEAU LOCAL =====
# Configuration pour servir React sur le réseau
HOST=0.0.0.0
PORT=3000

# ===== API BACKEND =====
# URL de l'API backend (réseau local)
REACT_APP_API_URL=http://192.168.1.62:5000

# URL pour Socket.io (même que l'API)
REACT_APP_SOCKET_URL=http://192.168.1.62:5000

# ===== AGORA CONFIGURATION =====
# Votre App ID Agora (sans certificat)
REACT_APP_AGORA_APP_ID=0b17dfbaa0e444ae8616069fbd5a7080
REACT_APP_AGORA_APP_ID_DEV=0b17dfbaa0e444ae8616069fbd5a7080
REACT_APP_AGORA_APP_ID_STAGING=0b17dfbaa0e444ae8616069fbd5a7080
REACT_APP_AGORA_APP_ID_PROD=0b17dfbaa0e444ae8616069fbd5a7080

# ===== DÉVELOPPEMENT =====
NODE_ENV=development

# Debug Agora (pour voir les logs détaillés)
REACT_APP_AGORA_DEBUG=true

# Optionnel: Token temporaire pour les tests
REACT_APP_AGORA_TEMP_TOKEN=

# ===== CONFIGURATION LOCALE (commentée) =====
# Pour le développement local uniquement
# REACT_APP_API_URL=http://localhost:5000
# REACT_APP_SOCKET_URL=http://localhost:5000
# HOST=localhost